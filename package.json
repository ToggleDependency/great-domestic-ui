{
  "name": "directory-form",
  "version": "0.0.1",
  "description": "Simple form to capture data",
  "main": "index.js",
  "private": true,
  "scripts": {
    "sass-watch": "sass --style expanded --line-numbers --load-path node_modules/govuk_frontend_toolkit/stylesheets --load-path node_modules/govuk-elements-sass/public/sass --watch ui/static/sass/main.scss:ui/static/css/main.css",
    "sass-dev": "sass --style expanded --line-numbers --load-path node_modules/govuk_frontend_toolkit/stylesheets --load-path node_modules/govuk-elements-sass/public/sass ui/static/sass/main.scss ui/static/css/main.css",
    "sass-prod": "sass --style compressed --load-path node_modules/govuk_frontend_toolkit/stylesheets --load-path node_modules/govuk-elements-sass/public/sass ui/static/sass/main.scss ui/static/css/main.css"
  },
  "author": "Graham Veal",
  "license": "ISC",
  "devDependencies": {
    "govuk-elements-sass": "^1.2.1",
    "govuk_frontend_toolkit": "^4.13.0"
  }
}
